# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Stripe Configuration
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-key-here

# Analytics Configuration
EXPO_PUBLIC_AMPLITUDE_API_KEY=your-amplitude-key-here

# Sentry Configuration
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn-here

# CI / Server tokens (DO NOT commit real values; keep these in local .env.local or in GitHub Actions / EAS secrets)
# EXPO_TOKEN is used by EAS/EAS builds and should be stored as an Actions secret or in your local .env.local only.
EXPO_TOKEN=your-expo-ci-token-here

# SUPABASE_SERVICE_ROLE_KEY must NEVER be included in client/mobile bundles.
# Store the service role key only in server-side environments or CI (e.g. GitHub Actions secrets).
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Sentry auth token used by CI/CD for release uploads. Keep as a CI secret, not in client bundles.
SENTRY_AUTH_TOKEN=your-sentry-auth-token-here

# Environment
EXPO_PUBLIC_ENVIRONMENT=development
