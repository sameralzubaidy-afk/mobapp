{
  "name": "EXUpdates",
  "version": "29.0.15",
  "summary": "Fetches and manages remotely-hosted assets and updates to your app's JS bundle.",
  "description": "Fetches and manages remotely-hosted assets and updates to your app's JS bundle.",
  "license": "MIT",
  "authors": "650 Industries, Inc.",
  "homepage": "https://docs.expo.dev/versions/latest/sdk/updates/",
  "platforms": {
    "ios": "15.1",
    "tvos": "15.1",
    "osx": "11.0"
  },
  "swift_versions": "5.9",
  "source": {
    "git": "https://github.com/expo/expo.git"
  },
  "static_framework": true,
  "dependencies": {
    "ExpoModulesCore": [],
    "React-Core": [],
    "EXStructuredHeaders": [],
    "EXUpdatesInterface": [],
    "EXManifests": [],
    "EASClient": [],
    "ReachabilitySwift": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-jsi": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  },
  "compiler_flags": "",
  "pod_target_xcconfig": {
    "GCC_TREAT_INCOMPATIBLE_POINTER_TYPE_WARNINGS_AS_ERRORS": "YES",
    "GCC_TREAT_IMPLICIT_FUNCTION_DECLARATIONS_AS_ERRORS": "YES",
    "DEFINES_MODULE": "YES",
    "SWIFT_COMPILATION_MODE": "wholemodule",
    "OTHER_CFLAGS[config=*Debug*]": "$(inherited) -DUSE_DEV_CLIENT=1",
    "OTHER_SWIFT_FLAGS[config=*Debug*]": "$(inherited) -DUSE_DEV_CLIENT",
    "OTHER_CFLAGS[config=*Release*]": "$(inherited)",
    "OTHER_SWIFT_FLAGS[config=*Release*]": "$(inherited)"
  },
  "user_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"${PODS_CONFIGURATION_BUILD_DIR}/EXUpdates/Swift Compatibility Header\""
  },
  "source_files": "EXUpdates/**/*.{h,m,swift}",
  "script_phases": {
    "name": "Generate updates resources for expo-updates",
    "script": "bash -l -c \"$PODS_TARGET_SRCROOT/../scripts/create-updates-resources-ios.sh\"",
    "execution_position": "before_compile"
  },
  "resource_bundles": {
    "EXUpdates": []
  },
  "exclude_files": "Tests/",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/*.{h,m,swift}",
      "resources": "Tests/Support/**/*",
      "dependencies": {
        "ExpoModulesTestCore": []
      },
      "pod_target_xcconfig": {
        "USER_HEADER_SEARCH_PATHS": "\"${CONFIGURATION_TEMP_DIR}/EXUpdates.build/DerivedSources\"",
        "GCC_TREAT_INCOMPATIBLE_POINTER_TYPE_WARNINGS_AS_ERRORS": "YES",
        "GCC_TREAT_IMPLICIT_FUNCTION_DECLARATIONS_AS_ERRORS": "YES",
        "DEFINES_MODULE": "YES",
        "SWIFT_COMPILATION_MODE": "wholemodule"
      }
    }
  ],
  "swift_version": "5.9"
}
