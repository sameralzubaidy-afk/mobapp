{
  "name": "p2p-kids-marketplace",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "test:ci": "jest --runInBand --reporters=default",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test-sentry": "node scripts/test-sentry-node.js",
    "eas:build:dev": "eas build --profile development --platform all --non-interactive",
    "eas:build:prod": "eas build --profile production --platform all --non-interactive",
    "sentry:create-release": "sentry-cli releases new $RELEASE",
    "sentry:upload-sourcemaps": "sentry-cli releases files $RELEASE upload-sourcemaps ./ --rewrite",
    "sentry:finalize-release": "sentry-cli releases finalize $RELEASE",
    "sentry:release": "npm run sentry:create-release && npm run sentry:upload-sourcemaps && npm run sentry:finalize-release",
    "postinstall": "patch-package",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules && npm install",
    "check:supabase": "node ./scripts/check-supabase.js",
    "e2e:build:ios": "npx expo prebuild --platform ios --no-install && xcodebuild -project ios/ios.xcodeproj -scheme p2p-kids-marketplace -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
    "e2e:run:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "npx expo prebuild --platform android --no-install && cd android && ./gradlew assembleDebug",
    "e2e:run:android": "detox test --configuration android.emu.debug"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.5.16",
    "@hookform/resolvers": "^3.10.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.4.1",
    "@sentry/react-native": "~7.2.0",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.87.0",
    "date-fns": "^3.6.0",
    "expo": "~54.0.27",
    "expo-application": "~7.0.8",
    "expo-constants": "~18.0.11",
    "expo-dev-client": "^6.0.20",
    "expo-device": "~8.0.10",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.9",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.14",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.15",
    "native-base": "^3.4.28",
    "react": "19.1.0",
    "react-hook-form": "^7.68.0",
    "react-native": "0.81.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^14.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "sentry-expo": "~7.0.0",
    "zod": "^3.25.76",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@sentry/cli": "^2.0.0",
    "@sentry/node": "^10.29.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.1.5",
    "@types/jest": "^29.5.4",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.6.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.6.4",
    "jest-expo": "^54.0.4",
    "patch-package": "^6.5.0",
    "prettier": "^3.7.4",
    "react-dom": "19.1.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "^5.3.3"
  },
  "private": true
}
